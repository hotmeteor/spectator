{
  "name": "hotmeteor/spectator",
  "description": "Testing helpers for your OpenAPI spec",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "spectator",
    "openapi",
    "testing"
  ],
  "authors": [
    {
      "name": "Adam Campbell",
      "email": "adam@hotmeteor.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "cebe/php-openapi": "^1.4",
    "laravel/framework": "^7.30.3|^8.24.0",
    "opis/json-schema": "^1.0"
  },
  "require-dev": {
    "laravel/sanctum": "^2.9",
    "nunomaduro/collision": "^4.0|^5.1",
    "orchestra/testbench": "^5.0|^6.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Spectator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spectator\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/sanctum"
      ],
      "providers": [
        "Spectator\\SpectatorServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
