{
  "name": "hotmeteor/spectator",
  "description": "Testing helpers for your OpenAPI spec",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "spectator",
    "openapi",
    "testing"
  ],
  "authors": [
    {
      "name": "Adam Campbell",
      "email": "adam@hotmeteor.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "devizzent/cebe-php-openapi": "^1.0",
    "laravel/framework": "^10.0 | ^11.0",
    "opis/json-schema": "^2.3"
  },
  "require-dev": {
    "larastan/larastan": "^2.8",
    "laravel/pint": "^1.13",
    "nunomaduro/collision": "^7.0|^8.0",
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Spectator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spectator\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs": "vendor/bin/pint",
    "test": " COLLISION_PRINTER=1 vendor/bin/phpunit --no-output",
    "analyse": "vendor/bin/phpstan analyse",
    "all": [
      "@cs",
      "@test",
      "@analyse"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "laravel": {
      "providers": [
        "Spectator\\SpectatorServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
